{
    "manifest_version": 2,
    "name": "Skrrrtify",
    "version": "2",
    "description": "Skrrrt everything.",
    "short_name": "Skrrr",
    "permissions": ["activeTab"],
    "background": {
      "scripts": ["skrrrt.js"]
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["skrrrt.js"]
      }
    ],
    "action": {},
    "browser_action": {},
    "web_accessible_resources": [
      "sounds/airhorn.mp3",
      "sounds/skrrrrrr.mp3"
  ]

   }

